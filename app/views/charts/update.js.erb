function newTitles() {
  const newTitles = "<%= j render 'charts/title_section', chart: @chart %>";
  const titles = document.getElementById('chart-titles');
  titles.innerHTML = newTitles;
}

function newNotes() {
  const newNotes = "<%= j @chart.notes %>";
  const notes = document.getElementById('notes-content');
  notes.innerHTML = newNotes;
}

